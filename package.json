{
  "name": "JueXue-Dao-App",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project ./",
    "start": "node ./dist/src/index.js",
    "build-run": "tsc --project ./ && yarn run dev",
    "watch": "nodemon -e ts -w ./src -x yarn run start:ts",
    "dev": "nodemon ./src/index.ts",
    "start:ts": "ts-node src/index.ts",
    "clean": "rmdir -r dist"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "mime": "^3.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.5.3",
    "morgan": "~1.9.1",
    "nft.storage": "^7.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.13",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "_moduleAliases": {
    "@": "dist/src",
    "@configs": "dist/configs"
  }
}
